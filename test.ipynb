    "n = 10\n",
    "mu = 5\n",
    "p = mu / n\n",
    "\n",
    "binom = torch.distributions.Binomial(total_count=n, probs=p)\n",
    "k = torch.arange(n + 1)\n",
    "pmf = torch.exp(binom.log_prob(k))\n",
    "print(pmf)"
